<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="operating_unit_form_view_inherit_casa" model="ir.ui.view">
        <field name="name">operating.unit.form.view.inherit.casa</field>
        <field name="model">operating.unit</field>
        <field name="inherit_id" ref="operating_unit.view_operating_unit_form" />
        <field name="arch" type="xml">
            <xpath expr="//field[@name='active']" position="after">
                <field name="pricelist_id" required="1" />
            </xpath>
        </field>
    </record>
</odoo>
